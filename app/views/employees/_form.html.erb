<% if @errors %>
<% @errors.each do |error| %>
<p><%= error %></p>
<% end %>
<% end %> 


<%= form_for @employee do |f| %>
<%= f.label :first_name, "Employee's first name:" %>
<%= f.text_field :first_name %>
<br>
<br>
<%= f.label :last_name, "Employee's last_name:" %>
<%= f.text_field :last_name %>
<br>
<br>
<%= f.label :alias, "Employee's alias:" %>
<%= f.text_field :alias %>
<br>
<br>
<%= f.label :title, "Employee's title:" %>
<%= f.text_field :title %>
<br>
<br>
<%= f.label :office, "Employee's office:" %>
<%= f.text_field :office %>
<br>
<br>
<%= f.label :img_url, "Employee's photo" %>
<%= f.text_field :img_url %>
<br>
<br>
<%= f.label :dog_id, "Employee's master" %>
<%= f.collection_select(:dog_id, @dogs, :id, :name) %>
<br>
<br>
<%= f.submit %>
<% end %>